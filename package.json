{
  "name": "textlint-plugin-latex2e",
  "version": "1.2.0",
  "description": "A textlint plugin for LaTeX2e",
  "main": "lib/index.js",
  "bin": {
    "tex2tast": "./bin/tex2tast"
  },
  "scripts": {
    "lint": "run-p lint:*",
    "lint:eslint": "eslint '**/*.{ts,js}'",
    "lint:prettier": "prettier --check '**/*.{ts,js,json}'",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint --fix '**/*.{ts,js}'",
    "fix:prettier": "prettier --write '**/*.{ts,js,json}'",
    "build": "tsc",
    "test": "jest",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm test && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "textlint",
    "latex"
  ],
  "author": "TANIGUCHI Masaya",
  "license": "MIT",
  "dependencies": {
    "@textlint/ast-node-types": "^12.0.0",
    "fp-ts": "^2.8.3",
    "latex-utensils": "^3.0.0"
  },
  "devDependencies": {
    "@textlint/ast-tester": "12.2.2",
    "@textlint/kernel": "12.2.2",
    "@textlint/textlint-plugin-markdown": "12.2.2",
    "@textlint/types": "12.2.2",
    "@types/jest": "28.1.8",
    "@types/node": "16.18.11",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "dictionary-en": "3.2.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "textlint-rule-spelling": "0.3.0",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "typescript-language-server": "1.2.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint-plugin-latex2e.git"
  },
  "bugs": {
    "url": "https://github.com/textlint/textlint-plugin-latex2e/issues"
  },
  "homepage": "https://github.com/textlint/textlint-plugin-latex2e#readme",
  "optionalDependencies": {
    "commander": "^9.0.0"
  }
}
