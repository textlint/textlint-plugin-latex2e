{
  "name": "textlint-plugin-latex2e",
  "version": "1.1.0",
  "description": "A textlint plugin for LaTeX2e",
  "main": "lib/index.js",
  "scripts": {
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./**/*.ts",
    "lint:prettier": "prettier --check ./**/*.ts",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint --fix ./**/*.ts",
    "fix:prettier": "prettier --write ./**/*.ts",
    "build": "tsc",
    "test": "jest",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm test"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "textlint",
    "latex"
  ],
  "author": "TANIGUCHI Masaya",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@textlint/ast-node-types": "^4.3.4",
    "fp-ts": "^2.8.3",
    "latex-utensils": "^3.0.0"
  },
  "devDependencies": {
    "@textlint/ast-tester": "^2.2.4",
    "@textlint/kernel": "^3.3.6",
    "@textlint/textlint-plugin-markdown": "^5.2.6",
    "@textlint/types": "^1.4.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.11.4",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "can-npm-publish": "^1.3.3",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "husky": "^4.3.8",
    "jest": "^26.5.0",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "textlint-rule-ginger": "^2.2.1",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "typescript-language-server": "^0.4.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint-plugin-latex2e.git"
  },
  "bugs": {
    "url": "https://github.com/textlint/textlint-plugin-latex2e/issues"
  },
  "homepage": "https://github.com/textlint/textlint-plugin-latex2e#readme"
}